{
  "name": "binary-tree-visualizer",
  "version": "2.1.5",
  "description": "A binary tree visualizer",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "prepare": "husky install",
    "dev": "tsc -p . --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc -p .",
